class Solution {
public:
    int asciiToInteger(string& str){
        int num = 0;
        for(int i=0;i<str.length();i++){
           num = num * 10 + (str[i] - '0');
        }
        return num;
    }
    int maximum69Number (int num) {
        string str = to_string(num);
        for(int i=0;i<str.length();i++){
            if(str[i] == '6'){
                str[i] = '9';
                break;
            }
        }
    return asciiToInteger(str);
    }
};
